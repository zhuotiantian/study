{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": false, //关闭console警告
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "variable-name": false,
    "no-namespace": false,
    "no-reference": false
  },
  "rulesDirectory": []
}
